{
  "expo": {
    "name": "Task Manager",
    "slug": "task-manager",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ronaldninoio.taskmanager"
    },
    "android": {
      "package": "com.ronaldninoio.taskmanager",
      "versionCode":1,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
         "ACCESS_FINE_LOCATION",
          "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "description": "App para gestionar tareas",
    "githubUrl": "https://github.com/ronaldnino-io/task-manager.git"
  }
}
